app:
  host: "localhost"
  env: "local"
  port: 9001
  code: "001" #service identity, must be unique among services, 

database:
  driver: "mysql"
  name: "{{ cookiecutter.app_name }}"
  user: "user"
  password: "password"
  host: "localhost"
  port: "3306"
  additionalParameters: "parseTime=true"

server:
  port: "9001"

jwt:
  expirationTime: "648000"
  secret: "secret_for_local"

Sentry:
  DSN: "[dsn-sentry]"

flipServer:
  baseUrl: "https://b2c-act-dev-box.flip.id/api/v2"

cache:
  isEnable: true
  host: "localhost"
  port: 6379
  db: 2
  password:
  dialConnTimeOut: 5s
  readTimeOut: 5s
  writeTimeOut: 5s
  idleTimeOut: 10s
  maxConnLifeTime: 5m
  maxIdleConn: 2
  maxActiveConn: 50
  isWait: true

errorMap:
  1001:
    id: "Error koneksi database"
    en: "Database connection error"
  1002:
    id: "Error koneksi cache"
    en: "Cache connection error"
